<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC
		"-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="demoMapper">
	<resultMap id="demo" type="com.mamahao.ass.dto.demo.Demo"></resultMap>
	<select id="findById" parameterType="Long" resultMap="demo">
		SELECT * FROM t_ass_demo
		<where>
			id=#{id}
		</where>
	</select>
	<insert id="insert" parameterType="com.mamahao.ass.dto.demo.Demo" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO t_ass_demo(id,name)
		VALUES (#{id},#{name})
	</insert>
</mapper>